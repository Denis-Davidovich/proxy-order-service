{
    "name": "app/order-service",
    "description": "Simple order service with REST and SOAP endpoints",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-soap": "*",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.16",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.5",
        "laminas/laminas-soap": "^2.14 || ^2.15",
        "laminas/laminas-validator": "^2.64",
        "symfony/cache": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dom-crawler": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/twig-bundle": "^7.3",
        "symfony/validator": "^7.0",
        "symfony/yaml": "^7.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "zircote/swagger-php": "^5.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "phpunit/phpunit": "^10.5",
        "symfony/browser-kit": "^7.0",
        "symfony/phpunit-bridge": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
